{
  "name": "@theo/messenger-poc",
  "version": "1.0.0",
  "main": "build/server.js",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint \"**/*.{js,jsx}\"",
    "lint:css": "stylelint \"**/*.css\"",
    "dev": "concurrently 'npm run dev:server' 'npm run dev:webpack'",
    "dev:server": "NODE_ENV=development babel-watch source/server.js",
    "dev:webpack": "NODE_ENV=development babel-node scripts/webpack-server.js",
    "storybook": "start-storybook -p 9001",
    "clean": "rm -rf build/ && mkdir build",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack",
    "start": "NODE_ENV=production node build/server.bundle.js",
    "debug": "NODE_ENV=production node --debug build/server.bundle.js",
    "tunnel": "babel-node scripts/tunnel.js"
  },
  "author": "Thomas Oberm√ºller",
  "license": "UNLICENSED",
  "devDependencies": {
    "@kadira/storybook": "^1.28.1",
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-watch": "^2.0.2",
    "chalk": "^1.1.3",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "json-loader": "^0.5.4",
    "localtunnel": "^1.8.1",
    "postcss-cssnext": "^2.5.2",
    "postcss-loader": "^0.9.1",
    "postcss-sorting": "^1.5.0",
    "style-loader": "^0.13.1",
    "stylelint": "^6.5.1",
    "stylelint-config-standard": "^8.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "chalk": "^1.1.3",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "react": "^15.1.0",
    "react-addons-update": "^15.1.0",
    "react-dom": "^15.1.0",
    "request-promise": "^3.0.0",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6"
  },
  "private": true
}
